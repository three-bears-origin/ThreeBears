<body>

  <header>
    <!--add linke later-->
    <a href="" class="logo">
      <img src="" alt="Logo">
    </a>
    <div class="search-bar">
      <!--add search bar later-->
      <h3>Search</h3>
    </div>
  </header>

  <div class="page">

    <div class="nav-bar">
      <ul class="nav-icons">
        <li class="nav-item">
          <!--Change to link later-->
          <h3>Profile</h3>
        </li>
        <li class="nav-item">
          <!--add link later-->
          <a href="">Timeline</a>
        </li>
        <li class="nav-item">
          <!--Change to link later-->
          <h3>Chat</h3>
        </li>
      </ul>
    </div>

    <div class="timeline-container"> 
      <h1> Your Timeline </h1>
      <ul class="timeline">
        <% @events.each do |event| %>
          <div class = "clear"></div>
          <li class="timeline-event">
            <div class="event-date"><%= event.date %></div>
            <div class="event-box">
              <div class="event-title"><%= event.title %></div>
              <img class="icon image" src="/assets/image-icon.svg">
              <img class="icon text" src="/assets/text-icon.svg">
              <img class="icon audio" src="/assets/audio-icon.svg">
              <img class="icon new-contribution" src="/assets/new-contribution-icon.svg">
            </div>
          </li>
        <% end %>
      </ul>
      <a class="new-event" href="<%=new_event_path%>">Create a new event</a>
      
    </div>

  </div>

</body>
